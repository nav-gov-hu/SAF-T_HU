	<xs:complexType name="SAFTHUHeaderStructure">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Az audit fájl általános adatai</xs:documentation>
			<xs:documentation xml:lang="en">General information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AuditFileVersion" type="SAFTcodeType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Audit fájl verziószáma</xs:documentation>
					<xs:documentation xml:lang="en">Identification of the Standard Audit File SAF-T version being used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuditFileDateCreated" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Audit fájl készítésének dátuma</xs:documentation>
					<xs:documentation xml:lang="en">Audit file creation date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuditID" type="SAFTlongtextType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">A NAV által adott megbízólevél szám</xs:documentation>
					<xs:documentation xml:lang="en">Audit Id provided by NAV</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TOCID" type="SAFTmiddle2textType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Az adóhivatali adatbekérés egyedi azonosítója, melyet az adóhivatal határoz meg.</xs:documentation>
					<xs:documentation xml:lang="en">Identifier of the Table of Contents File. Generated by the tax auth Information System.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SoftwareCompanyName" type="SAFTmiddle2textType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Az audit fájlt előállító szoftvert fejlesztő társaság megnevezése</xs:documentation>
					<xs:documentation xml:lang="en">Name of the software company whose product created the SAF-T</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SoftwareID" type="SAFTlongtextType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Az audit fájlt előállító szoftver megnevezése</xs:documentation>
					<xs:documentation xml:lang="en">Name of the software that generated the SAF-T</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SoftwareVersion" type="SAFTshorttextType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Audit fájlt előállító szoftver verziószáma</xs:documentation>
					<xs:documentation xml:lang="en">Version of the software that created the SAF-T</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DataSourceCompanyName" type="SAFTmiddle2textType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Azon szoftvergyártó cég neve, amely a számviteli programot, az ERP-t vagy a szoftvert fejlesztette ki, ahonnan az adatok érkeznek</xs:documentation>
					<xs:documentation xml:lang="en">Name of the software company that developed the accounting program, ERP or software from where the data is coming from</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DataSourceSystem" type="SAFTlongtextType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Könyvelőprogram vagy ERP megnevezése, melyből az adatok származnak</xs:documentation>
					<xs:documentation xml:lang="en">Accounting program, ERP or software name from where the data is coming from</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DataSourceSystemVersion" type="SAFTshorttextType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Számviteli program, ERP vagy szoftver verziószáma, ahonnan az adatok származnak</xs:documentation>
					<xs:documentation>Accounting program, ERP or software version from where the data is coming from</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Company" type="SAFTHUCompanyHeaderStructure">
				<xs:annotation>
					<xs:documentation xml:lang="hu">A társaság neve és a székhely címe</xs:documentation>
					<xs:documentation xml:lang="en">Company's name and headquarters address details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DefaultCurrencyCode" type="ISOCurrencyCode">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Könyvelés pénzneme (három karakterből álló pénznemkód az ISO 4217 szerint).
Pénznem kód, amelyet alapértelmezés szerint az ellenőrzési fájlban használunk a főkönyvi bejegyzés struktúrán. Például a magyar forint esetében - forint.</xs:documentation>
					<xs:documentation xml:lang="en">Three-character currency code according to ISO 4217.
Currency code that is used by default in the audit file on the general ledger entries structure. For example, in the case of Hungarian Forint - HUF.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExchangeRateMethod" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">A társaság által választott árfolyam meghatározásának módszere. Opciók:
						„AAHB" - A választott - hitelintézet által meghirdetett devizavételi és devizaeladási árfolyamának átlaga,
						„OHNB" - A  Magyar Nemzeti Bank által közzétett hivatalos devizaárfolyama,
						„OMER" - Valuta szabadpiaci árfolyama,
						„OECB" - Az Európai Központi Bank által közzétett hivatalos devizaárfolyama,
						„OT" - Egyéb
					</xs:documentation>
					<xs:documentation xml:lang="en">Exchange rate method used on the audit file, options:
						"AAHB" - Average of selling and buying rate of the account holder bank,
						"OHNB" - Official exchange rate of Hungarian National bank,
						"OMER" - Open market exchange rate,
						"OECB" - Official exchange rate of European Central bank,
						"OT" - Other
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="SAFTcodeType">
						<xs:enumeration value="AAHB"/>
						<xs:enumeration value="OHNB"/>
						<xs:enumeration value="OMER"/>
						<xs:enumeration value="OECB"/>
						<xs:enumeration value="OT"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SelectionCriteria" type="SAFTHUSelectionCriteriaStructure" minOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Audit fájl leválogatásának kritériumai</xs:documentation>
					<xs:documentation>Criteria for collecting Audit File</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HeaderComment" type="SAFTlongtextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Egyéb megjegyzések</xs:documentation>
					<xs:documentation xml:lang="en">Other comments</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SAFTHUInvoicePartnerStructure">
		<xs:annotation>
			<xs:documentation>A ki nem fizetett számlák partner struktúrája</xs:documentation>
		</xs:annotation>
		<xs:sequence>
