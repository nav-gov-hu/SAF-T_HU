	<xs:complexType name="SAFTHUFiscalCalendarStructure">
		<xs:annotation>
			<xs:documentation xml:lang="hu">A társasági üzleti év adatai</xs:documentation>
			<xs:documentation xml:lang="en">Fiscal accounting calendar of the company structure</xs:documentation>
		</xs:annotation>
