									</xs:element>
									<xs:element name="DueDateText" type="SAFTmiddle2textType">
										<xs:annotation>
											<xs:documentation>Fizetési határidő szöveg formátumban</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="PaymentDate" type="xs:date">
							<xs:annotation>
								<xs:documentation xml:lang="hu">Kiegyenlítés dátuma</xs:documentation>
								<xs:documentation xml:lang="en">A unique number assigned to the general ledger entry</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PaymentDelay" type="SAFTquantityType">
							<xs:annotation>
								<xs:documentation>Késedelmes napok száma</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PaymentMethod" type="SAFTHUPaymentMethodType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Fizetési módszer</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Description" type="SAFTlongtextType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="hu">Leírás, megjegyzés</xs:documentation>
								<xs:documentation xml:lang="en">Description or comment</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DebitAmount" type="AmountStructure">
							<xs:annotation>
								<xs:documentation xml:lang="hu">Tartozik összeg (bruttó)</xs:documentation>
								<xs:documentation xml:lang="en">Total gross sales value (including taxes and shipping costs) of the document</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CreditAmount" type="AmountStructure">
							<xs:annotation>
								<xs:documentation xml:lang="hu">Követel összeg (bruttó)</xs:documentation>
								<xs:documentation xml:lang="en">Unpaid ammount</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Balance" type="AmountStructure">
							<xs:annotation>
								<xs:documentation xml:lang="hu">Egyenleg összege (Bruttó)</xs:documentation>
								<xs:documentation xml:lang="en">Indicates whether the amounts is a debit or a credit amount (D – debit / C – credit)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
