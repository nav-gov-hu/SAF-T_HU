						<xs:documentation xml:lang="hu">Összeg a könyvelés pénznemétől eltérő pénznemben</xs:documentation>
						<xs:documentation xml:lang="en">Amount in foreign currency</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ExchangeRate" type="SAFTexchangerateType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="hu">Használt átváltási árfolyam. CurrencyAmount x ExchangeRate = Amount</xs:documentation>
						<xs:documentation xml:lang="en">Used exchange rate. CurrencyAmount x ExchangeRate = Amount</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
